{
    "manifest_version": 3,
    "name": "Wi-Fi Auto-Login",
    "version": "1.0",
    "description": "Automatically logs into the college Wi-Fi portal.",
  
    "permissions": [
      "storage",
      "scripting",
      "tabs"
    ],
  
    "host_permissions": 
      ["*://192.168.42.1/*"]
    ,
  
    "background": {
      "service_worker": "scripts/background.js"
    },
  
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
      }
    },
  
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
}